{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "target": "es2016",
    "module": "esnext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "jsx": "react-native"
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ]
}
